{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Global Suicide Rate",
    "width": 800,
    "height": 600,
    "data": {
      "url": "https://raw.githubusercontent.com/JinEnTan/vega-lite-homework-w9/json/ne_110m.json",
      "format": {
            "type": "topojson",
            "feature": "ne_110m.json"
          }
    },
    "transform": [{
        "lookup": "properties.NAME",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/JinEnTan/vega-lite-homework-w9/Dataset/master.csv"
          },
          "key": "country",
          "fields": ["suicides/100k pop"]
        }
      }
    ],
    "projection": {
      "type": "equirectangular"
    },
    "mark": {
      "type": "geoshape"
      },
    "encoding": {
      "color": {
        "field": "Active Cases",
        "type": "quantitative",
        "scale": {"type": "log"}
      },
      "tooltip": [
        {"field": "properties.NAME", "type": "nominal", "title": "Country"},
        {"field": "Active", "type": "quantitative"}
      ]
    }
  
  }